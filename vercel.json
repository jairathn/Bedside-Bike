{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api-build/index.js": {
      "runtime": "@vercel/node@3",
      "includeFiles": "api-build/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api-build/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
